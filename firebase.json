{
  "hosting": {
    "public": "dist/apps/music-ai-app/browser",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "api/**",
        "run": {
          "serviceId": "music-ai-server",
          "region": "southamerica-east1 "
        }
      }
    ]
  }
}
